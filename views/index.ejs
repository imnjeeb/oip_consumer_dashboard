  <div class="container">
    <div class="row">

      <!-- SWITCHES BLOCK -->
      <div class="col-sm-3 col-lg-3">
        <div class="dash-unit">
          <dtitle>MiLight Switches</dtitle>
          <hr>
          <div class="info-user">
            <span aria-hidden="true" class="li_bulb fs2"></span>
          </div>
          <br>
          <!-- switch start -->
          <div id="onSwitcher" class="switch">
            <input type="radio" class="switch-input" name="OnOff" value="off" id="off" checked="">
            <label for="off" class="switch-label switch-label-off">Off</label>
            <input type="radio" class="switch-input" name="OnOff" value="on" id="on">
            <label for="on" class="switch-label switch-label-on">On</label>
            <span class="switch-selection"></span>
          </div>
          <!-- switch end -->
          <!-- color start -->
          <div id="colorSwitcher" class="switch switch-blue">
            <input type="radio" class="switch-input" name="BlueWhite" value="white" id="white" checked="">
            <label for="white" class="switch-label switch-label-off">White</label>
            <input type="radio" class="switch-input" name="BlueWhite" value="blue" id="blue">
            <label for="blue" class="switch-label switch-label-on">Blue</label>
            <span class="switch-selection"></span>
          </div>
          <!-- color end -->
          <!-- auto start -->
          <div id="autoSwitcher" class="switch switch-yellow">
            <input type="radio" class="switch-input" name="AutoTemp" value="manual" id="manual" checked="">
            <label for="manual" class="switch-label switch-label-off">Manual</label>
            <input type="radio" class="switch-input" name="AutoTemp" value="auto" id="auto">
            <label for="auto" class="switch-label switch-label-on">Auto</label>
            <span class="switch-selection"></span>
          </div>
          <!-- auto end -->
        </div>
      </div>

      <!-- GRAPH CHART - lineandbars.js file -->
      <div class="col-sm-3 col-lg-3">
        <div class="dash-unit">
          <dtitle>Sensors Reading</dtitle>
          <hr>
          <div class="section-graph">
            <div id="importantchart"></div>
            <br>
            <div class="graph-info">
              <i id="temp-arrow" class="fa fa-caret-right"></i>
              <span id="info-temperature" class="graph-info-big"> </span>
              <span id="info-humidity" class="graph-info-small"> </span>
            </div>
          </div>
        </div>
      </div>
      <!-- Pole Knob -->
      <div class="col-sm-3 col-lg-3">
        <div class="dash-unit">
          <dtitle>Brightness Control</dtitle>
          <hr>
          <br>
          <br>
          <div class="knob">
            <input id="pole_knob" class="dial" data-width="105" data-height="105" data-min=0 data-max=100 value="0" data-angleOffset="-125" data-angleArc="250" autocomplete="off">
            <div class="btn-group">
              <a id="decr_brightness" class="btn btn-xs bg-yellow pole-dimmer" href="#">
                <i class="fa fa-minus" title="Dimmer"></i>
              </a>
              <a id="incr_brightness" class="btn btn-xs bg-yellow pole-dimmer" href="#">
                <i class="fa fa-plus" title="Brighter"></i>
              </a>
            </div>
        </div>
          <div class="cont">
            <p>MiLight Brightness Control</p>
          </div>
        </div>
      </div>

      <div class="col-sm-3 col-lg-3">

        <!-- LOCAL TIME BLOCK -->
        <div class="half-unit">
          <dtitle>Local Time</dtitle>
          <hr>
          <div class="clockcenter">
            <digiclock>12:45:25</digiclock>
          </div>
        </div>

        <!-- SERVER UPTIME -->
        <div class="half-unit">
          <dtitle>Server Uptime</dtitle>
          <hr>
          <div class="cont">
            <p><img src="./img/up.png" alt=""> <bold>Up</bold> | 356ms.</p>
          </div>
        </div>

      </div>
    </div><!-- /row -->


